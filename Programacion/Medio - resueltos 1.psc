Proceso Ejercicio1
	a<-9.8;
	Escribir "ingresar velocidad inical";
	Leer Vi;
	Escribir "ingresar tiempo";
	Leer t;
	Escribir "ingresar masa";
	Leer m;
	Escribir "ingresar altura";
	Leer h;
	Vf<-Vi+(a*t);
	Ec<-1/2*m*Vf^2;
	ht<-h-Vi*t-1/2*a*t^2;
	Escribir "la velocidad es: ",Vf," m/s";
	Escribir "la energia cinética es: ",Ec," J";
	Escribir "la altura es: ",ht," mts";
	
FinProceso
